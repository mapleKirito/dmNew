#IV_window {
	z-index:10001234;
	font: 12px Arial, Helvetica, sans-serif;
	color: #606373;
}

#IV_secondLine {
	font: 10px Arial, Helvetica, sans-serif;
	color:#606373;
}

#IV_window a {text-decoration:none;border:none;}


#IV_overlay {
	position: fixed;
	z-index:100;
	top: 0px;
	left: 0px;
	height:100%;
	width:100%;
}
#IV_window #IV_header #IV_toolbar a.IV_zoomB{background:url(../images/prevI.png) no-repeat;}
#IV_window #IV_header #IV_toolbar a.IV_zoomN{background:url(../images/nextI.png) no-repeat;}
#IV_window #IV_header #IV_toolbar a.IV_zoomIn{background:url(../images/inI.png) no-repeat;}
#IV_window #IV_header #IV_toolbar a.IV_zoomOut{background:url(../images/outI.png) no-repeat;}
#IV_window #IV_header #IV_toolbar a.IV_zoomAuto{background:url(../images/realI.png) no-repeat;}
#IV_window #IV_header #IV_toolbar a.IV_zoomFix{background:url(../images/repatI.png) no-repeat;}
.IV_overlayMacFFBGHack {background: url(macFFBgHack.png);}
.IV_overlayBG {
	background-color:rgb(232, 232, 232);
	filter:alpha(opacity=75);
	-moz-opacity: 0.75;
	opacity: 0.75;
}

* html #IV_overlay { /* ie6 hack */
     position: absolute;
     height: expression($("#windowheight",parent.document).val() > document.body.offsetHeight ? $("#windowheight",parent.document).val() : document.body.offsetHeight + 'px');
}

#IV_window {
	position: fixed;
	background:rgb(232, 232, 232);
	z-index: 10001234;
	color:#000000;
	display:none;
	border: 0px solid #606373;
	text-align:center;
	top:50%;
	left:48%;
	overflow:hidden;
	width:100%;
}

* html #IV_window { /* ie6 hack */
position: absolute;
margin-top: expression(0 - parseInt(this.offsetHeight / 2) + (TBWindowMargin = document.documentElement && document.documentElement.scrollTop || document.body.scrollTop) + 'px');
}

#IV_window img#IV_Image {
	display:block;
	border-right: 0px solid #ccc;
	border-bottom: 0px solid #ccc;
	border-top: 0px solid #666;
	border-left: 0px solid #666;
	cursor:pointer;
	z-index:100;
	overflow: hidden;
	position : absolute;
}

#IV_window #IV_header{
	height:45px;
	background:rgb(232, 232, 232);
	margin:0px;
	z-index:101;
	position:relative;
	display:block;
	border-bottom:0px solid #606373;
}

#IV_caption{
	height:14px;
	padding:7px 30px 3px 25px;
	float:left;
	/* width:200px; */
}

#IV_toolbar{
    width:500px;
    height:35px;
	margin:0px;
	margin-left:20px;
	margin-top:0px;
	float:left;
}
#IV_toolbar a{
    height:40px;
    width:40px;
}
.IV_ico{
	display:block;
	width:25px; height:25px;
	float:left;
	margin:3px 3px;
	cursor:pointer;
	border:0px solid #606373;
}
.IV_ico:hover,.IV_ico:focus{border:0px solid #606373;}

#IV_closeWindow{
	height:35px;
	width:40px;
	float:right;
	margin-right:20px;
	cursor:pointer;	
	background:url(../images/error.png) no-repeat;

}

#IV_closeAjaxWindow{
	padding:7px 10px 5px 0;
	margin-bottom:1px;
	text-align:right;
	float:right;
}

#IV_ajaxWindowTitle{
	float:left;
	padding:7px 0 5px 10px;
	margin-bottom:1px;
}

#IV_title{
	background-color:rgb(232, 232, 232);
	height:27px;
}

#IV_ajaxContent{
	clear:both;
	padding:2px 15px 15px 15px;
	overflow:auto;
	text-align:left;
	line-height:1.4em;
}

#IV_ajaxContent.IV_modal{
	padding:15px;
}

#IV_ajaxContent p{
	padding:5px 0px 5px 0px;
}

#IV_load{
	position: fixed;
	/* background:url(../images/loadingAnimation.gif) top no-repeat; */
	display:none;
	height:100px;
	width:100px;
	z-index:103;
	top: 50%;
	left: 50%;
	margin: -50px 0 0 -50px; /* -height/2 0 0 -width/2 */
}

* html #IV_load { /* ie6 hack */
position: absolute;
margin-top: expression(0 - parseInt(this.offsetHeight / 2) + (TBWindowMargin = document.documentElement && document.documentElement.scrollTop || document.body.scrollTop) + 'px');
}

#IV_HideSelect{
	z-index:99;
	position:fixed;
	top: 0;
	left: 0;
	background-color:rgb(232, 232, 232);
	border:none;
	filter:alpha(opacity=0);
	-moz-opacity: 0;
	opacity: 0;
	height:100%;
	width:100%;
}

* html #IV_HideSelect { /* ie6 hack */
     position: absolute;
     height: expression($("#windowheight",parent.document).val() > document.body.offsetHeight ? $("#windowheight",parent.document).val() : document.body.offsetHeight + 'px');
}

#IV_iframeContent{
	clear:both;
	border:none;
	margin-bottom:-1px;
	margin-top:1px;
	_margin-bottom:1px;
}