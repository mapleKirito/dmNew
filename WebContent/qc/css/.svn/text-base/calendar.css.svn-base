/* Reset */
.ui-datepicker,
.ui-datepicker table,
.ui-datepicker tr,
.ui-datepicker td,
.ui-datepicker th {
	margin: 0;
	padding: 0;
	border: none;
	border-spacing: 0;
}

/* Calendar Wrapper */
.ui-datepicker {
	display: none;
	width: 380px;
    padding-bottom:15px;
	cursor: default;

	text-transform: uppercase;
	font-family: Tahoma;
	font-size: 12px;

	background: #ae9b84;	
}

/* Calendar Header */
.ui-datepicker-header {
    height:75px;
    line-height:75px;
    font-size:18px;
    font-weight:bold;
	position: relative;
	border-bottom: 1px solid #beaf9d;
}

.ui-datepicker-title { text-align: center; }

/* Month */
.ui-datepicker-month {
	position: relative;
	padding-right: 5px;
	color: #FFFFFF;
}

/* Year */
.ui-datepicker-year {
	color: #FFFFFF;
}

/* Prev Next Month */
.ui-datepicker-prev,
.ui-datepicker-next {
	position: absolute;
	top: -2px;
	padding: 5px;
	cursor: pointer;
}

.ui-datepicker-prev {
	left: 0;
	padding-left: 0;
}

.ui-datepicker-next {
	right: 0;
	padding-right: 0;
}

.ui-datepicker-prev span,
.ui-datepicker-next span{
	display: block;
	width: 25px;
	height: 25px;
	text-indent: -9999px;
    margin-top:22px;

	background-image: url(../images/icon-spriter.png);
}

.ui-datepicker-prev span { margin-left:22px; background-position: -75px -500px; }
.ui-datepicker-next span { margin-right:22px; background-position: -75px -550px; }
.ui-datepicker-prev-hover span { background-position: -75px -500px; }
.ui-datepicker-next-hover span { background-position: -75px -550px; }

.ui-datepicker-calendar { border-collapse:collapse; position:relative; width:90%; left:5%;}

/* Calendar "Days" */
.ui-datepicker-calendar th {
	padding-top: 25px;
	padding-bottom: 15px;
    font-size:14px;
    font-weight:bold;
	
	text-align: center;
	color: #FFFFFF;
}

.ui-datepicker-calendar td {
	padding: 2px 7px;
	
	text-align: center;
	line-height: 35px;

    font-size:14px;
}

.ui-datepicker-calendar .ui-state-default {
	display: block;
	width: 31px;
    height:31px;
    line-height:31px;
    text-align:center;
	outline: none;
    font-size:14px;

	text-decoration: none;
	color: #FFFFFF;
}

    .ui-datepicker-calendar .ui-state-default:hover {
        background:url(../images/icon-spriter.png) 0px -650px no-repeat;
    }

/* Day Active State*/
.ui-datepicker-calendar .ui-state-active,
.ui-datepicker-calendar .ui-state-active:hover {
    background:url(../images/icon-spriter.png) 0px -700px no-repeat;
}

/* today*/
.ui-datepicker-today .ui-state-default,
.ui-datepicker-today .ui-state-active,
.ui-datepicker-today .ui-state-default:hover,
.ui-datepicker-today .ui-state-active:hover {
    background:url(../images/icon-spriter.png) 0px -600px no-repeat;
	color: #88361f;
    font-weight:bold;
}

/* Other Months Days*/
.ui-datepicker-other-month .ui-state-default { color: #88361f; }

